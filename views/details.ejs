<html>
    <body>
        <H2>Booking Details</H2><hr>
        <p>restaurant_id: <%= booking.restaurant_id %></p>
        <p>name: <%= booking.name %></p>
        <p>borough: <%= booking.borough %></p>
        <p>cuisine: <%= booking.cuisine %></p>
        <p>street: <%= booking.address.street %></p>
        <p>building: <%= booking.address.building %></p>
        <p>zipcode: <%= booking.address.zipcode %></p>
        <p>coord: <%= booking.address.coord %></p>        
        <p>owner: <%= booking.address.owner %></p>   
        <% if (booking.photo) { %>
            <img src="data:image/jpg;base64, <%= booking.photo %>"><br>
        <% } %>

        <a href="/edit?_id=<%= booking._id %>">edit</a><br>
        <a href="/delete?_id=<%= booking._id %>">delete</a><br>
        <a href="/">home</a>
    </body>
</html>